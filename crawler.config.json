{
	"index_name": "tiahui-tech",
	"start_urls": [
		"https://www.tiahui.tech/docs/intro"
	],
	"sitemap_urls": [
		"https://www.tiahui.tech/sitemap.xml"
	],
	"sitemap_alternate_links": true,
	"stop_urls": [],
	"selectors": {
		"lvl0": {
			"selector": "//nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text()",
			"type": "xpath",
			"global": true,
			"default_value": "Documentation"
		},
		"lvl1": {
			"selector": "article h1",
			"type": "css"
		},
		"lvl2": {
			"selector": "article h2",
			"type": "css"
		},
		"lvl3": {
			"selector": "article h3",
			"type": "css"
		},
		"lvl4": {
			"selector": "article h4",
			"type": "css"
		},
		"lvl5": {
			"selector": "article h5",
			"type": "css"
		},
		"lvl6": {
			"selector": "article h6",
			"type": "css"
		},
		"text": {
			"selector": "article p, article li, article td",
			"type": "css"
		}
	},
	"strip_chars": " .,;:#",
	"custom_settings": {
		"separatorsToIndex": "_",
		"attributesForFaceting": [
			"language",
			"version",
			"type",
			"docusaurus_tag"
		],
		"attributesToRetrieve": [
			"hierarchy",
			"content",
			"anchor",
			"url",
			"url_without_anchor",
			"type"
		]
	}
}